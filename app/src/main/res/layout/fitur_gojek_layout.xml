<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/gosend_image_view"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginStart="24dp"
        android:background="@drawable/fitur_backgroud"
        android:backgroundTint="@color/soft_green"
        android:src="@drawable/gosend_bg_icon"
        app:layout_constraintStart_toEndOf="@+id/gofood_image_view"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <TextView
        android:id="@+id/gosend_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="36dp"
        android:text="@string/gosend"
        app:layout_constraintStart_toEndOf="@+id/gofood_textView"
        app:layout_constraintTop_toBottomOf="@+id/gosend_image_view" />

    <TextView
        android:id="@+id/etc_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="32dp"
        android:layout_marginTop="7dp"
        android:text="@string/lainnya"
        app:layout_constraintStart_toEndOf="@+id/checkin_textView"
        app:layout_constraintTop_toBottomOf="@+id/lainnya_image_view" />

    <ImageView
        android:id="@+id/gofood_image_view"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginStart="32dp"
        android:background="@drawable/fitur_backgroud"
        android:src="@drawable/gofood_bg_icon"
        app:layout_constraintStart_toEndOf="@+id/gocar_image_view"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gofood_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="56dp"
        android:text="@string/gofood"
        app:layout_constraintStart_toEndOf="@+id/gocar_textView"
        app:layout_constraintTop_toBottomOf="@+id/gofood_image_view" />

    <TextView
        android:id="@+id/checkin_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="44dp"
        android:text="@string/check_in"
        app:layout_constraintStart_toEndOf="@+id/gopulsa_textView"
        app:layout_constraintTop_toBottomOf="@+id/checkin_image_view" />

    <ImageView
        android:id="@+id/checkin_image_view"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/fitur_backgroud"
        android:src="@drawable/checkin_bg_icon"
        app:layout_constraintStart_toEndOf="@+id/gopulsa_image_view"
        app:layout_constraintTop_toBottomOf="@+id/gofood_textView" />

    <ImageView
        android:id="@+id/lainnya_image_view"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/etc_background"
        android:backgroundTint="@color/gray"
        android:padding="10dp"
        android:src="@drawable/etc_icon"
        app:layout_constraintStart_toEndOf="@+id/checkin_image_view"
        app:layout_constraintTop_toBottomOf="@+id/gosend_textView" />

    <ImageView
        android:id="@+id/goride_image_view"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:src="@drawable/goride_bg_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/gocar_image_view"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginStart="32dp"
        android:src="@drawable/gocar_bg_icon"
        app:layout_constraintStart_toEndOf="@+id/goride_image_view"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gocar_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="48dp"
        android:text="@string/gocar"
        app:layout_constraintStart_toEndOf="@+id/goride_textView"
        app:layout_constraintTop_toBottomOf="@+id/gocar_image_view" />

    <ImageView
        android:id="@+id/gomart_image_view"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/gomart_bg_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/goride_textView" />

    <ImageView
        android:id="@+id/gopulsa_image_view"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/gopulsa_bg_icon"
        app:layout_constraintStart_toEndOf="@+id/gomart_image_view"
        app:layout_constraintTop_toBottomOf="@+id/gocar_textView" />
    <TextView
        android:id="@+id/gomart_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="12dp"
        android:text="@string/gomart"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gomart_image_view" />
    <TextView
        android:id="@+id/gopulsa_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="36dp"
        android:text="@string/gopulsa"
        app:layout_constraintStart_toEndOf="@+id/gomart_textView"
        app:layout_constraintTop_toBottomOf="@+id/gopulsa_image_view" />
    <TextView
        android:id="@+id/goride_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="4dp"
        android:text="@string/goride"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/goride_image_view" />


</androidx.constraintlayout.widget.ConstraintLayout>